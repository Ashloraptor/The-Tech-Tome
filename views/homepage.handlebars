<!-- Loop over each object in the projects array -->
{{#each blogpost as |blogpost| }}
<div class="row mb-4 blogpost">
    <div class="col-md-5">
        <h2>
            <a href="/blogpost/{{blogpost.id}}">{{blogpost.name}}</a>
        </h2>
        <p>

        </p>
    </div>
    <div class="col-md-7">
        <p>
            {{blogpost.description}}
        </p>
    </div>
</div>
{{//each}}